[core]
    autocrlf = input
[user]
    name = Mark Sommers
    email = mark_sommers@apple.com
[filter "lfs"]
    required = true
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
[pull]
    rebase = false
[push]
    autoSetupRemote = true
[merge]
    tool = nvimdiff
